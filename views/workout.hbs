{{#each EXERCISE}}
<div class="row">
	<div class="flip-card">
	  <div class="flip-card-inner">
	    <div class="flip-card-front">
	       <h5 class="card-title">{{this.exercise}}</h5>
		        <p class="card-text">
		        	Focus Area: {{this.bodyPart}}
	       		 	Repetitions: {{this.reps}}
	        		# of Sets: {{this.sets}}
		      	</p>
		        <a href="#" class="btn btn-primary">Tutorial</a>
	    </div>
	    <div class="flip-card-back">
	      <iframe width="560" height="315" src={{this.video}} frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
	  </div>
	</div>
</div>
{{/each}}
 